{
  "name": "source-editors-ep",
  "version": "0.0.1",
  "description": "element-plus全家桶源码编辑器，支持内网、less、ts、element图标、自定义格式化等",
  "packageManager": "pnpm@8.14.0",
  "type": "module",
  "main": "dist/ssr-stub.js",
  "module": "dist/code-opt.js",
  "files": [
    "dist"
  ],
  "types": "dist/code-opt.d.ts",
  "exports": {
    ".": {
      "types": "./dist/code-opt.d.ts",
      "import": "./dist/code-opt.js",
      "require": "./dist/ssr-stub.js"
    },
    "./monaco-editor": {
      "types": "./dist/monaco-editor.d.ts",
      "import": "./dist/monaco-editor.js",
      "require": null
    },
    "./style.css": "./dist/code-opt.css",
    "./dist/style.css": "./dist/code-opt.css"
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./*"
      ]
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build-preview": "vite build -c vite.preview.config.ts",
    "format": "prettier --write .",
    "release": "bumpp --all",
    "version": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "prepublishOnly": "npm run build"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm exec lint-staged --concurrent false"
  },
  "lint-staged": {
    "*": [
      "prettier --write --cache --ignore-unknown"
    ]
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "wangzunmin",
  "license": "MIT",
  "bugs": {
    "url": ""
  },
  "homepage": "",
  "devDependencies": {
    "@babel/types": "^7.23.6",
    "@rollup/plugin-replace": "^5.0.5",
    "@types/less": "^3.0.6",
    "@types/node": "^20.10.6",
    "@vitejs/plugin-vue": "^5.0.2",
    "@volar/cdn": "~1.11.1",
    "@volar/monaco": "~1.11.1",
    "@volar/typescript": "~1.11.1",
    "@vue/compiler-sfc": "^3.2.45",
    "@vue/language-service": "1.9.0-alpha.3",
    "dayjs": "^1.11.10",
    "eslint-plugin-vue": "^9.20.1",
    "hash-sum": "^2.0.0",
    "less": "^4.2.0",
    "lint-staged": "^15.2.0",
    "monaco-editor-core": "^0.45.0",
    "node-stdlib-browser": "^1.2.0",
    "prettier": "^2.8.8",
    "shikiji": "^0.9.17",
    "shikiji-monaco": "^0.9.17",
    "simple-git-hooks": "^2.9.0",
    "sucrase": "^3.35.0",
    "typescript": "^5.3.3",
    "vite": "^5.0.11",
    "vite-plugin-dts": "^3.7.0",
    "vite-plugin-node-stdlib-browser": "^0.2.1",
    "vue": "^3.4.5",
    "vue-eslint-parser": "^9.4.2",
    "vue-tsc": "1.9.0-alpha.3"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.1",
    "element-plus": "^2.5.3",
    "prettier-plugin-vue": "^1.1.6"
  }
}
